<%
	getReadableDate = function(dateString) {
		const date = new Date(dateString);
		const readableDateString = date.toLocaleString("en-us", {
			day: "numeric",
			month: "short",
			year: "numeric",
			hour: "2-digit",
			minute: "numeric",
			hour12: false,
			timeZoneName: "short",
		});
		return readableDateString;
	};
%>
